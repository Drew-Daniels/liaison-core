{
  "name": "liaison-core",
  "version": "0.0.6",
  "author": "Drew Daniels",
  "description": "A simple messaging library meant to enable easy CORS communication between windows and embedded iframes - using the browser postMessage API.",
  "license": "MIT",
  "type": "module",
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "jsdom": "^22.0.0",
    "typescript": "^5.0.2",
    "vitest": "^0.31.0"
  },
  "packageManager": "yarn@3.5.1",
  "repository": {
    "type": "git",
    "url:": "https://github.com/Drew-Daniels/liaison-core"
  },
  "bugs": {
    "url": "https://github.com/Drew-Daniels/liaison-core/issues"
  },
  "keywords": [
    "postMessage",
    "iframe",
    "embedded",
    "javascript",
    "cors"
  ]
}
